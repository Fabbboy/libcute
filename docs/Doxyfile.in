PROJECT_NAME = "libcute"
PROJECT_NUMBER = "@PROJECT_VERSION@"
OUTPUT_DIRECTORY = docs
INPUT = ../include ../lib @CONFIG_H@ ../docs/mainpage.md
FILE_PATTERNS = *.h *.c *.md
RECURSIVE = YES
USE_MDFILE_AS_MAINPAGE = ../docs/mainpage.md
GENERATE_LATEX = NO
WARN_IF_UNDOCUMENTED = NO
QUIET = NO
HTML_COLORSTYLE = DARK
HTML_COLORSTYLE_HUE = 30
HAVE_DOT = YES
CALL_GRAPH = YES
CALLER_GRAPH = YES
DOT_MULTI_TARGETS = YES
